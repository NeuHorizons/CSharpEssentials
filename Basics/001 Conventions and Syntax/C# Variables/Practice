using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
        // add words to say your name or age so it can write it out
      string name = "Cameron";
      String Age = "19";
      Console.WriteLine(name);  
      Console.WriteLine(Age);
     }
  }
}

using System;

namespace MyApplication
{
  class Program
  {
    // makes it so you can reffer to the my num in the writing Line
    static void Main(string[] args)
    {
      int myNum = 1500;
      Console.WriteLine(myNum);
     }
  }
}

using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
         // makes it so you can reffer to the code in a differnt spot
      int myNum;
      myNum = 1500;
      Console.WriteLine(myNum);
     }
  }
}
using System;

namespace MyApplication
{
  class Program
  {
    // overwrite the code to int myNum
    static void Main(string[] args)
    {
      int myNum = 15;
      myNum = 1500;
      Console.WriteLine(myNum);
    }
  }
}