      myNum = 1500;
      Console.WriteLine(myNum);
     }
  }
}
using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      int myNum = 15;
      myNum = 1500;
      Console.WriteLine(myNum);
    }
  }
}